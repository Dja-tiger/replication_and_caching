{
  "dashboard": {
    "id": null,
    "uid": null,
    "title": "E-Commerce Metrics",
    "tags": ["ecommerce", "cache", "api"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 0,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "type": "graph",
        "title": "Cache Hit Rate",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "rate(cache_hits_total[5m])",
            "refId": "A",
            "legendFormat": "Cache Hits/s"
          },
          {
            "expr": "rate(cache_misses_total[5m])",
            "refId": "B",
            "legendFormat": "Cache Misses/s"
          }
        ],
        "yaxes": [
          {"format": "short", "show": true},
          {"format": "short", "show": true}
        ],
        "xaxis": {"show": true}
      },
      {
        "id": 2,
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "type": "stat",
        "title": "Total Cache Hits",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "cache_hits_total",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "graphMode": "area",
          "justifyMode": "center"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            },
            "color": {"mode": "thresholds"},
            "unit": "short"
          }
        }
      },
      {
        "id": 3,
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "type": "graph",
        "title": "API Request Rate",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum by (endpoint, method) (rate(http_requests_total[5m]))",
            "refId": "A",
            "legendFormat": "{{method}} {{endpoint}}"
          }
        ],
        "yaxes": [
          {"format": "reqps", "show": true},
          {"format": "short", "show": true}
        ],
        "xaxis": {"show": true}
      },
      {
        "id": 4,
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "type": "graph",
        "title": "API Response Time",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum by (endpoint, le) (rate(http_request_duration_seconds_bucket[5m])))",
            "refId": "A",
            "legendFormat": "p95 {{endpoint}}"
          },
          {
            "expr": "histogram_quantile(0.50, sum by (endpoint, le) (rate(http_request_duration_seconds_bucket[5m])))",
            "refId": "B",
            "legendFormat": "p50 {{endpoint}}"
          }
        ],
        "yaxes": [
          {"format": "s", "show": true},
          {"format": "short", "show": true}
        ],
        "xaxis": {"show": true}
      },
      {
        "id": 5,
        "gridPos": {"x": 0, "y": 16, "w": 8, "h": 6},
        "type": "stat",
        "title": "Total API Requests",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(http_requests_total)",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "graphMode": "none",
          "justifyMode": "center"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null}
              ]
            },
            "color": {"mode": "thresholds"},
            "unit": "short"
          }
        }
      },
      {
        "id": 6,
        "gridPos": {"x": 8, "y": 16, "w": 8, "h": 6},
        "type": "stat",
        "title": "Cache Hit Ratio",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "cache_hits_total / (cache_hits_total + cache_misses_total) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "graphMode": "none",
          "justifyMode": "center"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 80}
              ]
            },
            "color": {"mode": "thresholds"},
            "unit": "percent",
            "decimals": 2
          }
        }
      },
      {
        "id": 7,
        "gridPos": {"x": 16, "y": 16, "w": 8, "h": 6},
        "type": "stat",
        "title": "Active WebSocket Connections",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "websocket_connections_active",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "auto",
          "graphMode": "none",
          "justifyMode": "center"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null}
              ]
            },
            "color": {"mode": "thresholds"},
            "unit": "short"
          }
        }
      }
    ],
    "time": {"from": "now-30m", "to": "now"},
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  },
  "overwrite": true
}